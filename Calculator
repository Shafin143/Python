a=int(input())
b=int(input()) 

c=[1,2,3,4]
d=["Addition","Minus","Multiplication","Division"]

i=0
j=0

while i<=3 and j<=3:
  print(c[i],".",d[j])
  i=i+1
  j=j+1

f = int(input())

w = a+b
x = a-b
y = a*b
z = a/b

m = [w,x,y,z]
l=[1,2,3,4]

g=0


while g<=3:
  if f == l[g]:
    print(m[g])
    g=g+1
  else:
    
    g=g+1
